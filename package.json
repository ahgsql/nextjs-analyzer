{
  "name": "nextjs-analyzer",
  "version": "2.2.2",
  "description": "A modular tool that comprehensively analyzes Next.js projects. Includes component, performance, security, SEO, data fetching, code quality, and historical analysis features.",
  "main": "src/index.js",
  "bin": {
    "nextjs-analyzer": "./bin/nextjs-analyzer.js"
  },
  "scripts": {
    "start": "node bin/nextjs-analyzer.js",
    "analyze": "node bin/nextjs-analyzer.js analyze",
    "analyze:component": "node bin/nextjs-analyzer.js analyze:component",
    "analyze:performance": "node bin/nextjs-analyzer.js analyze:performance",
    "analyze:security": "node bin/nextjs-analyzer.js analyze:security",
    "analyze:seo": "node bin/nextjs-analyzer.js analyze:seo",
    "analyze:data-fetching": "node bin/nextjs-analyzer.js analyze:data-fetching",
    "analyze:code-quality": "node bin/nextjs-analyzer.js analyze:code-quality",
    "analyze:routing": "node bin/nextjs-analyzer.js analyze:routing",
    "analyze:history": "node bin/nextjs-analyzer.js analyze:history",
    "visualize": "node bin/nextjs-analyzer.js analyze --module visualization",
    "list-modules": "node bin/nextjs-analyzer.js list-modules"
  },
  "keywords": [
    "nextjs",
    "analyzer",
    "server-component",
    "client-component",
    "performance",
    "security",
    "seo",
    "code-quality",
    "routing",
    "data-fetching",
    "visualization",
    "history",
    "trend-analysis",
    "app-router",
    "pages-router",
    "next13",
    "next14",
    "react"
  ],
  "author": "ahgsql",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "inquirer": "^8.2.5"
  },
  "devDependencies": {
    "@babel/parser": "^7.23.0",
    "@babel/traverse": "^7.23.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ahgsql/nextjs-analyzer"
  },
  "bugs": {
    "url": "https://github.com/ahgsql/nextjs-analyzer/issues"
  },
  "homepage": "https://github.com/ahgsql/nextjs-analyzer#readme"
}
